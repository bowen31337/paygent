{
  "version": 2,
  "builds": [
    {
      "src": "src/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/main.py"
    },
    {
      "src": "/docs",
      "dest": "src/main.py"
    },
    {
      "src": "/redoc",
      "dest": "src/main.py"
    },
    {
      "src": "/openapi.json",
      "dest": "src/main.py"
    },
    {
      "src": "/api/v1/ws",
      "dest": "src/main.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "functions": {
    "src/main.py": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "regions": ["sfo1"],
  "framework": null
}